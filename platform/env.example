# Database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_DB_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# Authentication
CLERK_PUBLISHABLE_KEY=pk_test_your-publishable-key
CLERK_SECRET_KEY=sk_test_your-secret-key

# Observability
SENTRY_DSN_WEB=https://your-dsn@sentry.io/project-id
SENTRY_DSN_API=https://your-dsn@sentry.io/project-id
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=

# Deployment
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-org-id
VERCEL_PROJECT_ID=your-project-id
EXPO_TOKEN=your-expo-token

# API Configuration
API_BASE_URL=https://your-api.vercel.app
WEB_BASE_URL=https://your-web.vercel.app
MOBILE_BASE_URL=https://your-mobile.vercel.app

# Security
JWT_SECRET=your-jwt-secret-min-32-chars
ENCRYPTION_KEY=your-encryption-key-32-chars

# Rate Limiting
REDIS_URL=redis://localhost:6379
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_BURST=100

